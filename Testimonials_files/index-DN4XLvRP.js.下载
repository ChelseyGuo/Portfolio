  if (!__vite__mapDeps.viteFileDeps) {
    __vite__mapDeps.viteFileDeps = ["assets/ImageEditor-B6uizD3H.js","assets/ImageEditor-DsXv3XeY.css","assets/ImageResizer-Dkb6M4sG.js","assets/ImageResizer-DcIGOfGR.css","assets/Analtyics-CPIbRB-7.js","assets/Analtyics-DRBU8ESC.css"]
